{
  "name": "fullstacklearning",
  "version": "1.0.0",
  "description": "This is a test directory for bootstrap and node",
  "main": "index.html",
  "scripts": {
    "start": "concurrently \"npm run watch-scss\" \"npm run lite\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch-scss": "onchange \"css/*.scss\" -- npm run scss",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "imagemin": "imagemin img/* -o dist/img",
    "usemin": "usemin contactus.html -d dist --htmlmin -o dist/contactus.html && usemin aboutus.html -d dist --htmlmin -o dist/aboutus.html && usemin index.html -d dist --htmlmin -o dist/index.html",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myousuf07/fullstacklearning.git"
  },
  "author": "Mohammed Yousuf P U",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/myousuf07/fullstacklearning/issues"
  },
  "homepage": "https://github.com/myousuf07/fullstacklearning#readme",
  "devDependencies": {
    "browser-sync": "^2.23.6",
    "concurrently": "^4.1.0",
    "cssmin": "^0.4.3",
    "grunt": "^1.0.3",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^2.2.1",
    "grunt-contrib-htmlmin": "^2.4.0",
    "grunt-contrib-imagemin": "^2.0.1",
    "grunt-contrib-uglify": "^3.3.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-sass": "^2.1.0",
    "grunt-usemin": "^3.1.1",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "htmlmin": "0.0.7",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.4.0",
    "node-sass": "^4.11.0",
    "onchange": "^5.2.0",
    "rimraf": "^2.6.3",
    "time-grunt": "^1.4.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "bootstrap": "^4.1.2",
    "bootstrap-social": "^5.1.1",
    "fontawesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3"
  }
}
